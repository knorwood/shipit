{
  "manifest_version": 2,

  "name": "shipit",
  "description": put a boat on it",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/pull/*"],
      "js": ["boatbutton.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://github.com/*/pull/*"
  ]
}
